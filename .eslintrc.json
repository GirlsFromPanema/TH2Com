{
	"parser": "babel-eslint",
	"extends": "mars/node",
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"env": {
		"node": true
	},
	"rules": {
		"no-negated-condition": "off",
		"prefer-destructuring": "off",
		"quotes": [
			"error",
			"single"
		],
		"quote-props": "off",
		"no-unused-vars": "off"
	}
}